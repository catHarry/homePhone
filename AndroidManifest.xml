<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.homephonenew"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />
    
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.homephonenew.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
  
        <!-- WetActivity -->
        <activity
            android:theme="@style/newstyle"
            android:name="com.homephonenew.WetActivity">
        </activity>
        
        <!-- BingxiangActivity -->
        <activity
            android:theme="@style/newstyle"
            android:name="com.homephonenew.BingxiangActivity">
        </activity>
        
        <!-- SetActivity -->
        <activity
            android:theme="@style/newstyle"
            android:name="com.homephonenew.SetActivity">
        </activity>
        
        <!-- kongtiaoActivity -->
        <activity
            android:theme="@style/newstyle"
            android:name="com.homephonenew.KongtiaoActivity">
        </activity>
        
        <!-- LightActivity -->
        <activity
            android:theme="@style/newstyle"
            android:name="com.homephonenew.LightActivity">
        </activity>
        
        <!-- HealthActivity -->
        <activity
            android:theme="@style/newstyle"
            android:name="com.homephonenew.HealthActivity">
        </activity>
        
        <!-- VideoActivity -->
        <activity
            android:theme="@style/newstyle"
            android:name="com.homephonenew.VideoActivity">
        </activity>
        
    </application>

</manifest>
